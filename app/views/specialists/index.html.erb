  <%= params %>
  <!-- main div  -->
  <div class="wrapper-blue">
    <h4 class="text-center">All specialists in your selection</h4>
    <br>
    <div class="container">
      <!-- splitsen in 2 delen (searchbox & result lijst) -->
      <div class="row">
        <div class="col-xs-6">
          <!-- kaarten rechts -->
          <div class="row">
            <% @specialists.each do |specialist| %><!-- no puts -->
            <!-- kaart renderen met specialist -->
            <%= render 'shared/card', specialist: specialist %>
            <% end %>
          </div>
        </div>
        <div class="col-xs-6">
          <!-- searchbox links -->
            <div class="row">
              <%= render 'shared/searchbox' %>
            </div>
        </div>
      </div>
    </div>
  </div>
<!-- </div> -->


  <!-- Including Bootstrap JS (with its jQuery dependency) so that dynamic components work -->


